<android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
	xmlns:app="http://schemas.android.com/apk/res-auto"
	xmlns:tools="http://schemas.android.com/tools"
	android:layout_width="match_parent"
	android:layout_height="wrap_content"
	android:focusable="true"
	android:clickable="true"
	android:orientation="vertical"
	tools:layout_editor_absoluteY="81dp"
	tools:layout_editor_absoluteX="0dp">

	<TextView

		android:textStyle="bold"
		android:fontFamily="sans-serif-condensed"
		tools:targetApi="jelly_bean"
		android:id="@+id/northingList"
		android:textColor="@color/colorPressMDAS"

		android:layout_width="0dp"
		android:layout_height="wrap_content"
		android:layout_marginStart="8dp"
		android:layout_marginEnd="8dp"
		app:layout_constraintTop_toTopOf="@+id/avatar"
		android:layout_marginTop="0dp"
		app:layout_constraintLeft_toRightOf="@+id/avatar"
		android:layout_marginLeft="0dp" />

	<TextView

		android:textStyle="bold"
		android:fontFamily="sans-serif-condensed"
		tools:targetApi="jelly_bean"
		android:textColor="@color/colorPressMDAS"
		android:id="@+id/eastingList"
		android:layout_width="0dp"
		android:layout_height="wrap_content"
		android:layout_marginStart="8dp"
		android:layout_marginEnd="8dp"
		app:layout_constraintBottom_toBottomOf="@+id/avatar"
		android:layout_marginBottom="0dp"
		app:layout_constraintLeft_toRightOf="@+id/avatar"
		android:layout_marginLeft="0dp" />

	<TextView
		android:textAllCaps="true"
		android:layout_width="35dp"
		android:layout_height="35dp"
		android:layout_gravity="center"
		android:gravity="center"
		android:text="P"
		android:textSize="16sp"
		android:id="@+id/avatar" android:layout_marginStart="8dp"
		app:layout_constraintLeft_toLeftOf="parent" android:layout_marginLeft="8dp"
		android:layout_marginTop="8dp" app:layout_constraintTop_toTopOf="parent"
		android:layout_marginBottom="8dp" app:layout_constraintBottom_toBottomOf="parent"
		app:layout_constraintVertical_bias="0.571" />

	<TextView

		android:textStyle="bold"
		android:fontFamily="sans-serif-condensed"
		tools:targetApi="jelly_bean"
		android:id="@+id/position"
		android:textColor="@color/colorPressMDAS"
		android:layout_width="wrap_content"
		android:layout_height="wrap_content"
		tools:ignore="RtlHardcoded"
		android:layout_marginRight="8dp"
		app:layout_constraintRight_toRightOf="parent"
		app:layout_constraintTop_toTopOf="parent"
		android:layout_marginTop="8dp" />

</android.support.constraint.ConstraintLayout>